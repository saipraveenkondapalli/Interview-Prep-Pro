{
  "manifest_version": 3,
  "name": "LeetCode Extension",
  "version": "1.0",
  "description": "LeetCode Extension-- test",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]

    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
 "web_accessible_resources": [
  {
    "resources": ["script.js", "content.js", "test.html", "leetcode.js", "leetcodeOld.js", "old.css"],
    "matches": ["<all_urls>"]
  }

]
}

