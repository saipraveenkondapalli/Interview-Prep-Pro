{
  "manifest_version": 3,
  "name": "Custom Sidebar Extension",
  "version": "1.0",
  "description": "Injects a custom sidebar into any website",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Custom Sidebar Extension"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
